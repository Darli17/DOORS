wait(1)
Character.Humanoid:TakeDamage(Damage)
wait(1)
Character.Humanoid:TakeDamage(Damage)
wait(1)
Character.Humanoid:TakeDamage(Damage)
wait(1)
Character.Humanoid:TakeDamage(Damage)
wait(1)
Character.Humanoid:TakeDamage(Damage)
wait(1)
Character.Humanoid:TakeDamage(Damage)


Frostbite.Part.Ambience:Stop()

Frostbite.Part.Attachment.Heylois.Enabled = false
