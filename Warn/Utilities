    game.Players.LocalPlayer:SetAttribute("Hidden", false)
        RightArm.Name = "RightUpperArm"
        LeftArm.Name = "LeftUpperArm"
        
        RightArm.RightShoulder.C1 = RightC1
        LeftArm.LeftShoulder.C1 = LeftC1
end)
shadow.Activated:Connect(function(plr)
    local effects = game:GetObjects("rbxassetid://94134410389484")[1]
    effects.Parent = workspace
    effects.Anchored = true
    local mouse = game.Players.LocalPlayer:GetMouse()
    workspace.Effects.CFrame = mouse.Target.CFrame
    local wthesigma = Instance.new("Attachment")
    wthesigma.Parent = mouse.Target
    workspace.Effects.Top.Top.Attachment1 = wthesigma
    game.ReplicatedStorage.Sounds["LA_The Courtyard"]:Play()
