local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local Window = Rayfield:CreateWindow({
   Name = "DOORS Utilities Gui",
  Icon = 12011843673,
  Theme = "Amethyst",
   LoadingTitle = "Loading...",
   LoadingSubtitle = "by ValerikRob",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but  you change the key, they will be unable to use your script
      GrabKeyFromSite = false, --  this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})
local Tab1 = Window:CreateTab("Entities", 12549578506)
local Tab12 = Window:CreateTab("Entities Every Time", 12549578506)
local Tab13 = Window:CreateTab("Entities Every Door", 12549578506)
local Tab2 = Window:CreateTab("Items", 12656753493)
local Tab3 = Window:CreateTab("Modes", 11247557290)
local TabR = Window:CreateTab("Player", 12812166882)
local TabG = Window:CreateTab("Main", 12801280877)
local Tab4 = Window:CreateTab("Visual", 12801280877)
local TabPS = Window:CreateTab("Extra", 6663675885)

local Button = Tab1:CreateButton({
   Name = "Rush",
   Callback = function()
--=== Varaibles --===
local plr = game.Players.LocalPlayer
local char = plr.Character
local hum = char.Humanoid
local humRoot = char.HumanoidRootPart

--=== Entity Settings ===--

local Entity = {
	Model = "rbxassetid://86181759655390",
	Speed = 3,
	WaitTime = 3,
	Repeats = nil
}

--=== Entity ===--
local EntityM = game:GetObjects(Entity.Model)[1]
local LatestRoom = game.ReplicatedStorage.GameData.LatestRoom.Value
EntityM.Parent = workspace
LatestRoom -= 1
EntityM.RushNew.CFrame = workspace.CurrentRooms[LatestRoom].RoomStart.CFrame
warn("Entity Arrived!")

wait(Entity.WaitTime)
local tweenInfo = TweenInfo.new(Entity.Speed, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
LatestRoom += 2
local tween = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomEnd.CFrame})
tween:Play()
wait(5)
EntityM:Destroy()
warn("Tween Stopped!")
--=== Credits ===--

--[[

	Darli_17 = Creator (Owner), Scripter.
	Vinuxy = Idea.
	
	Thanks!

]]

   end,
})

local Button = Tab1:CreateButton({
   Name = "Ambush",
   Callback = function()
--=== Varaibles --===
local plr = game.Players.LocalPlayer
local char = plr.Character
local hum = char.Humanoid
local humRoot = char.HumanoidRootPart

--=== Entity Settings ===--

local Entity = {
	Model = "rbxassetid://96669039243756",
	Speed = 1,
	WaitTime = 3,
	Repeats = nil
}

--=== Entity ===--
local EntityM = game:GetObjects(Entity.Model)[1]
local LatestRoom = game.ReplicatedStorage.GameData.LatestRoom.Value
EntityM.Parent = workspace
LatestRoom -= 1
EntityM.RushNew.CFrame = workspace.CurrentRooms[LatestRoom].RoomStart.CFrame
warn("Entity Arrived!")

wait(Entity.WaitTime)
local tweenInfo = TweenInfo.new(Entity.Speed, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
LatestRoom += 2
local tween = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomEnd.CFrame})
LatestRoom -= 3
local tween2 = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomEnd.CFrame})
tween:Play()
wait(2)
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(2)
EntityM:Destroy()
warn("Tween Stopped!")
