local Button = Tab3:CreateButton({
   Name = "A-60",
   Callback = function()
--=== Varaibles --===
local plr = game.Players.LocalPlayer
local char = plr.Character
local hum = char.Humanoid
local humRoot = char.HumanoidRootPart

--=== Entity Settings ===--

local Entity = {
	Model = "rbxassetid://88733963344547",
	Speed = 3,
	WaitTime = 3,
	Repeats = nil
}

--=== Entity ===--
local EntityM = game:GetObjects(Entity.Model)[1]
local LatestRoom = game.ReplicatedStorage.GameData.LatestRoom.Value
EntityM.Parent = workspace
LatestRoom -= 1
EntityM.RushNew.CFrame = workspace.CurrentRooms[LatestRoom].RoomEntrance.CFrame
warn("Entity Arrived!")

EntityM.RushNew.PlaySound:Clone().Parent = char.HumanoidRootPart
EntityM.RushNew.PlaySound:Play()

wait(Entity.WaitTime)
local tweenInfo = TweenInfo.new(Entity.Speed, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
LatestRoom += 2
local tween = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomExit.CFrame})
tween:Play()
EntityM.RushNew.Footsteps:Clone().Parent = char.HumanoidRootPart
EntityM.RushNew.Footsteps:Play()
wait(3)
EntityM:Destroy()
char.HumanoidRootPart.PlaySound:Destroy()
char.HumanoidRootPart.Footsteps:Destroy()
warn("Tween Stopped!")
--=== Credits ===--

--[[

	Darli_17 = Creator (Owner), Scripter.
	Vinuxy = Idea.
	
	Thanks!

]]

   end,
})
