local Button = Tab1:CreateButton({
   Name = "Ambush",
   Callback = function()
--=== Varaibles --===
local plr = game.Players.LocalPlayer
local char = plr.Character
local hum = char.Humanoid
local humRoot = char.HumanoidRootPart

--=== Entity Settings ===--

local Entity = {
	Model = "rbxassetid://96669039243756",
	Speed = 1,
	WaitTime = 3,
	Repeats = nil
}

--=== Entity ===--
local EntityM = game:GetObjects(Entity.Model)[1]
local LatestRoom = game.ReplicatedStorage.GameData.LatestRoom.Value
EntityM.Parent = workspace
LatestRoom -= 1
EntityM.RushNew.CFrame = workspace.CurrentRooms[LatestRoom].RoomEntrance.CFrame
warn("Entity Arrived!")

wait(Entity.WaitTime)
local tweenInfo = TweenInfo.new(Entity.Speed, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
LatestRoom += 2
local tween = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomExit.CFrame})
LatestRoom -= 3
local tween2 = game:GetService("TweenService"):Create(EntityM.RushNew, tweenInfo, {CFrame = workspace.CurrentRooms[LatestRoom].RoomExit.CFrame})
tween:Play()
wait(2)
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(3)
LatestRoom += 2
tween:Play()
wait(2)
LatestRoom -= 3
tween2:Play()
wait(2)
EntityM:Destroy()
warn("Tween Stopped!")
